{
  "name": "vanilla-image-cropper",
  "version": "1.3.2",
  "description": "A lightweight javascript imagecropper",
  "main": "imagecrop.js",
  "author": {
    "name": "Peter Vermeulen",
    "email": "nmb.peterv@gmail.com",
    "url": "https://github.com/peterver"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/peterver/vanilla-image-cropper.git"
  },
  "bugs": {
    "url": "https://github.com/peterver/vanilla-image-cropper/issues"
  },
  "keywords": [
    "imagecropper",
    "js",
    "html5",
    "lightweight"
  ],
  "files": [
    "dist/",
    "docs/"
  ],
  "scripts": {
    "lint": "eslint src/**/*.js",
    "format": "npm run lint -- --fix",
    "clean": "rimraf lib",
    "prebuild": "npm run lint && npm run clean",
    "build:js": "babel src --out-file lib/imagecrop.js",
    "build:css": "node-sass ./src/sass -o lib --output-style compressed",
    "build": "mkdir lib && npm run build:js && npm run build:css",
    "docs": "rimraf lib-docs && mkdir lib-docs && browserify docs/app.js > lib-docs/imagecrop-docs.js && node-sass ./docs -o lib-docs",
    "start": "browser-sync start --index docs/index.html --server --files docs lib",
    "test": "cross-env NODE_ENV=test jest",
    "tdd": "npm run test -- --watch",
    "check-coverage": "cross-env CI=true NODE_ENV=test jest --coverage"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.18.8",
    "browserify": "^16.2.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-better": "^0.1.5",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-require-in-package": "^1.0.3",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-rollup": "^2.16.2",
    "gulp-sass": "^4.0.2",
    "gulp-sass-lint": "^1.4.0",
    "jest": "^23.6.0",
    "node-sass": "^4.10.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-stream": "^1.24.1"
  }
}
